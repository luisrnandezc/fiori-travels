{
  "compilerOptions": {
    "target": "es2023",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "rootDir": "webapp",
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "mock/travels/*": ["webapp/mock/travels/*"]
    }
  },
  "include": ["webapp/**/*"]
}
