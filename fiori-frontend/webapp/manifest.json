{
  "_version": "1.65.0",
  "sap.app": {
    "id": "mock.travels",
    "type": "application",
    "title": "Fiori mock travels app",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "https://fiori-travels-api.onrender.com/odata/v4/travel/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "mock.travels.view.Dashboard",
      "type": "XML",
      "async": true,
      "id": "Dashboard"
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        }
      }
    }
  }
}
