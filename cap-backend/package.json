{
  "name": "cap-backend",
  "version": "1.0.0",
  "description": "SAP CAP Backend for Fiori Travels Project",
  "repository": "https://github.com/luisrnandezc/fiori-travels",
  "license": "MIT",
  "dependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds": "^9",
    "cors": "^2.8.6",
    "express": "^4"
  },
  "scripts": {
    "start": "npx cds-serve"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": ":memory:"
        }
      },
      "auth": {
        "kind": "dummy"
      }
    },
    "server": {
      "impl": "srv/server.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "private": true
}
